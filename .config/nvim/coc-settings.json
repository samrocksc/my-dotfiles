{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 30,
  "suggest.enablePreview": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "todolist.enable": true,
  "todolist.autoUpload": true,
  "todolist.notify": "floating",
  "markdownlint.config": {
    "line_length": false
  },
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": true,
  "tsserver.implicitProjectConfig.checkJs": false,
"
